# Flask Configuration
PORT=8000
FLASK_ENV=production  # Set to 'development' for development mode

# Cache Configuration
CACHE_TYPE=SimpleCache  # Options: SimpleCache, RedisCache, etc.
CACHE_TIMEOUT=60  # Cache lifetime in seconds

# MicroStrategy Connection
MSTR_URL_API=http://your-mstr-server:8080/MicroStrategyLibrary/api
MSTR_BASE_URL=http://your-mstr-server:8080
MSTR_USERNAME=your_username
MSTR_PASSWORD=your_password
MSTR_PROJECT=your_project_name

# Security
# Comma-separated list of valid API keys (leave empty to disable API key auth)
API_KEYS=your_api_key_1,your_api_key_2
RATE_LIMIT=100  # Maximum requests per period
RATE_LIMIT_PERIOD=60  # Period in seconds

# Logging
ENABLE_PERFORMANCE_LOGGING=true  # Enable detailed performance logging
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL